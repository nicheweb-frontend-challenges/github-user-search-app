er .theme-wrapper {\n  display: flex;\n  align-items: center;\n  justify-content: space-between;\n}\n\n/* nav */\nnav {\n  position: relative;\n}\nnav input {\n  font-size: 1.3rem;\n  line-height: 2.5rem;\n  width: 100%;\n  height: 60px;\n  padding-left: 45px;\n  padding-right: 97px;\n  background-image: url(\"assets/icon-search.svg\");\n  background-repeat: no-repeat;\n  background-position: left 16px center;\n  background-size: 20px auto;\n  text-overflow: ellipsis;\n}\ninput::placeholder {\n  opacity: 1;\n}\nnav button {\n  color: #ffffff;\n  background: #0079ff;\n  width: 84px;\n  height: 46px;\n  font-size: 1.4rem;\n  font-weight: 700;\n  line-height: 2.1rem;\n  position: absolute;\n  left: calc(100% - 97px);\n  margin: 6.5px;\n}\n\n/* ||Profile picture */\naside {\n  width: 70px;\n  height: 70px;\n  float: left;\n  margin-right: 20px;\n  background-color: #ffffff;\n  border-radius: 50%;\n}\n\n/* ||Main */\nmain header {\n  margin-bottom: 32px;\n}\nmain header .user-name {\n  font-weight: 400;\n  margin-bottom: 6px;\n}\nmain header .join-date {\n  font-weight: 400;\n}\nmain .bio {\n  font-size: 1.3rem;\n}\n\nmain table {\n  width: 100%;\n  text-align: center;\n  margin: 23px 0 24px;\n}\ntable thead {\n  margin-bottom: 8px;\n}\n\n/* ||List */\nul {\n  margin: 0;\n  padding: 0;\n  list-style: none;\n  display: grid;\n  gap: 1.6rem;\n}\nul path {\n  fill: currentColor !important;\n}\nul li:nth-of-type(1) svg {\n  margin-right: 0.625rem;\n}\nli {\n  display: grid;\n  grid-template-columns: 20px auto;\n  gap: 20px;\n}\nli h4 {\n  font-weight: 400;\n}\n"},"lineCount":null}},"error":null,"hash":"1fcbde700c88a564cc65c6a7d69421d5","cacheData":{"env":{}}}